{
    "id": "ant-template",
    "name": "Ant, Hibernate and Tomcat",
    "source": "git://github.com/CloudBees-community/ant-template.git",
    "description": "This is a basic java web application showcasing Hibernate ORM, all built with Ant",
    "doc-url": "https://github.com/CloudBees-community/ant-template/blob/master/README.md",
    "icon": "https://raw.github.com/CloudBees-community/ant-template/master/icon.png",
    "app-package": "http://cloudbees-downloads.s3.amazonaws.com/clickstart/hibernate_basic_ant.war",
    "disabled": true,
    "runtime": {
        "type": "tomcat"
    },
    "databases": [
        {
            "type": "mysql",
            "alias": "ExampleDS"
        }
    ],
    "build-with-jenkins": {
        "template": {
            "type": "https://raw.github.com/benjaminsavoy/ant-template/master/config.xml"
        }
    }
}
